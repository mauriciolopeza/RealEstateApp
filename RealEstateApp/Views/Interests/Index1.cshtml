@model IEnumerable<RealEstateApp.Models.Interests>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InterestName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkCompanies)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InterestName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkCompanies.CompanyName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InterestsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InterestsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InterestsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
